{
  "manifest_version": 2,
  "name": "keymacs",
  "version": "1.0",
  "description": "Emacs like keybind for Chrome",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["keymacs.js"],
      "run_at": "document_end"
    }
  ],
  "homepage_url": "http://midnightsuyama.org"
}
